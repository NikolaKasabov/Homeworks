{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off",
    "max-len": "off"
  }
}